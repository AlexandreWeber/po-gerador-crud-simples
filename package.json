{
    "name": "po-gerador-crud-simples",
    "publisher": "alexandre.dalabona",
    "displayName": "PO Gerardor de CRUD Simples",
    "description": "Gerador de CRUD Simples utilizando Angular + Portinari",
    "icon": "assets/images/po-icon.png",
    "homepage": "https://github.com/AlexandreWeber/po-gerador-crud-simples",
    "repository": {
        "type": "git",
        "url": "https://github.com/AlexandreWeber/po-gerador-crud-simples.git"
    },
    "keywords": [
        "ANGULAR",
        "Portinari",
        "CRUD"
    ],
    "version": "1.0.0",
    "activationEvents": [
        "onCommand:extension.poCRUDSimple"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.poCRUDSimple",
                "title": "PO: CRUD Simple"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "extension.poCRUDSimple",
                    "group": "po@1"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "@types/node": "^10.5.2",
        "typescript": "^2.0.3",
        "vscode": "^1.0.0"
    },
    "dependencies": {
        "change-case": "^3.0.0",
        "fs": "0.0.1-security",
        "fs-extra": "^0.30.0",
        "rxjs": "^5.0.0-rc.1"
    }
}
